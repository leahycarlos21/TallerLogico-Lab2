module problema1(input [3:0] in,
	output [6:0] out );
	
	unique case(in)
	4'b0000 :$display("cero");
	4'b0001 :$display("1");
	4'b0010 :$display("2");
	4'b0011 :$display("3");
	4'b0100 :$display("4");
	4'b0101 :$display("5");
	4'b0110 :$display("6");
	4'b0111 :$display("7");
	4'b1000 :$display("8");
	4'b1001 :$display("9");
	4'b1010 :$display("10");
	4'b1011 :$display("11");
endmodule
